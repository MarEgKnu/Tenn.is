@page
@model Tennis.Pages.Lanes.LaneCreateModel
@{
    ViewData["Title"] = "LaneCreate";
}

<h1>Create Lane</h1>
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div class="form-group">
                <label asp-for="@Model.NewLane.Id" class="control-label"></label>
                <input asp-for="@Model.NewLane.Id" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.NewLane.Outdoor" class="control-label"></label>
                <input asp-for="@Model.NewLane.Outdoor" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.NewLane.PadelTennis" class="control-label"></label>
                <input asp-for="@Model.NewLane.PadelTennis" class="form-control" />
            </div>

            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
                <input type="submit" value="Cancel" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>